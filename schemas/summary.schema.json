{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SpeedCloudflareCli Summary Results",
  "type": "object",
  "properties": {
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label":        { "type": "string" },
          "server_city":  { "type": "string" },
          "ip":           { "type": "string" },
          "latency":      { "type": "number" },
          "jitter":       { "type": "number" },
          "download":     { "type": "number" },
          "upload":       { "type": "number" }
        },
        "required": [
          "label",
          "server_city",
          "ip",
          "latency",
          "jitter",
          "download",
          "upload"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": ["results"],
  "additionalProperties": false
}
